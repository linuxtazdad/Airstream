           //Agent sent first message\n            _log('First agent message', 'INFO');\n            _googleAnalytics.sendEvent(GOOGLE_ANALYTICS_EVENTS.CHAT_AGENT_FIRST_MESSAGE.NAME, GOOGLE_ANALYTICS_EVENTS.CHAT_AGENT_FIRST_MESSAGE.CATEGORY);\n        }\n    }\n\n    function _log(message, type) {\n        if (window.lpTag && window.lpTag.log) {\n            window.lpTag.log(message, type, _name);\n        }\n    }\n\n    var _googleAnalytics = (function () {\n        var _optionalTrackingId;\n        var _trackingResolvingAttempts = 0; \n        var _hitQueue = [];\n        var _HIT_TYPES = { PAGE_VIEW: 0, EVENT: 1 };\n        var _sendPageView;\n        var _sendEvent;\n\n        function initialize(optionalTrackingId) {\n            try {\n                _log('Starting initialization of Google Analytics for tracking ID: ' + _caoTrackingId, 'INFO');\n                \n                //need to initialize for both cao trackingid and optional tracking id\n                _runWhenConditionIsMet(_tryTagManagerTracking, _isDocumentReady);\n\n                if (optionalTrackingId && optionalTrackingId !== '') {\n                    _log('Starting initialization of Google Analytics for optional tracking ID: ' + optionalTrackingId, 'INFO');\n                    _optionalTrackingId = optionalTrackingId;\n                } else {\n                    _log('No optional Google Analytics Tracking ID provided', 'INFO');\n                }\n            } catch (error) {\n                _log('Google Analytics initialization exception: ' + error.message, 'ERROR');\n            }\n        }\n\n        // These functions will (at first) queue up hits until Google Analytics is ready. \n        // Once ready, these functions are overwritten with the actual reporting logic\n        var sendPageView = function () { \n            try { _sendPageView(); } \n            catch(error) { _log('Google Analytics page view exception: ' + error.message, 'ERROR'); } \n        };\n        var se