     function _runWhenConditionIsMet(successCallback, conditionFunction, failureCallback, numberOfAttempts) {\n            try { \n                failureCallback = failureCallback || function () {};\n                numberOfAttempts = numberOfAttempts || 1000;\n                if (conditionFunction()) {\n                    successCallback();\n                } else if (numberOfAttempts > 0) {\n                    numberOfAttempts--;\n                    setTimeout(function() { _runWhenConditionIsMet(successCallback, conditionFunction, failureCallback, numberOfAttempts); }, 50);\n                } else {\n                    failureCallback();\n                }\n            } catch (error) { \n                _log('Google Analytics exception: ' + error.message, 'ERROR'); \n            }\n        }\n\n        function _loadScript(url, successCallback, errorCallback) {\n            lpTag.taglets.jsLoader.loadJS( {\n                loadObj: { url: url },\n                success: successCallback,\n                error: errorCallback,\n                context: window\n            });\n        }\n\n        function _isDocumentReady() {\n            return document.readyState === 'complete' || document.readyState === 'loaded';\n        }\n\n        function _log(message, type) {\n            if (window.lpTag && window.lpTag.log) {\n                window.lpTag.log(message, type, _name);\n            }\n        }\n\n        return {\n            initialize: initialize,\n            sendPageView: sendPageView,\n            sendEvent: sendEvent\n        };\n    })();\n\n    return {\n        v: _version,\n        name: _name,\n        init: init,\n        start: start,\n        getTrackingId: getTrackingId,\n        getCaoTrackingId: getCaoTrackingId,\n        includes: [\n\t\t\t{ name: \"lpUnifiedWindow\" },\n\t\t\t{ name: \"jsLoader\" }\n        ]\n    };\n})();","parameters":{"caoTrackingId":"UA-123581995-2","trackingId":""}}],"retry":false});